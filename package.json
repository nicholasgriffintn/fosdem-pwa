{
  "name": "fosdem-pwa",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:worker": "remix-pwa build",
    "dev": "run-p dev:*",
    "dev:remix": "remix dev --manual -c \"npm run start\"",
    "dev:worker": "remix-pwa dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "typecheck": "tsc",
    "pages:deploy": "npm run build && wrangler pages deploy ./public",
    "migrations:gen": "drizzle-kit generate:sqlite",
    "migrations:up": "drizzle-kit up:sqlite",
    "local:migrations:apply": "wrangler d1 migrations apply fosdem --local",
    "local:d1:init": "npm run local:migrations:apply && wrangler d1 execute fosdem --file='./fixtures/data.sql' --local",
    "migrations:apply": "wrangler d1 migrations apply fosdem",
    "d1:new": "wrangler d1 create fosdem",
    "d1:init": "npm run migrations:apply && wrangler d1 execute fosdem --file='./fixtures/data.sql'"
  },
  "dependencies": {
    "@epic-web/cachified": "^4.0.0",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@remix-pwa/cache": "^2.0.12",
    "@remix-pwa/client": "^2.0.1",
    "@remix-pwa/push": "^1.0.9",
    "@remix-pwa/strategy": "^2.1.9",
    "@remix-pwa/sw": "^2.1.12",
    "@remix-pwa/sync": "^2.0.1",
    "@remix-run/cloudflare": "^2.5.0",
    "@remix-run/cloudflare-pages": "^2.5.0",
    "@remix-run/css-bundle": "^2.5.0",
    "@remix-run/react": "^2.5.0",
    "@remix-run/serve": "^2.5.0",
    "autoprefixer": "^10.4.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.3",
    "isbot": "^4.1.0",
    "lru-cache": "^10.1.0",
    "lucide-react": "^0.317.0",
    "random-words": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-resizable-panels": "^1.0.9",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@remix-pwa/dev": "2.0.31",
    "@remix-pwa/worker-runtime": "^2.0.8",
    "@remix-run/dev": "^2.5.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "drizzle-kit": "^0.20.13",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^10.3.10",
    "npm-run-all2": "^6.0.0",
    "remix-pwa": "^3.0.19",
    "typescript": "^5.1.6",
    "wrangler": "^3.8.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}