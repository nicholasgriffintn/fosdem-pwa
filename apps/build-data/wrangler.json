{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "fosdem-pwa-build-data",
  "main": "src/index.ts",
  "compatibility_date": "2025-09-02",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "workers_dev": false,
  "preview_urls": false,
  "observability": {
    "enabled": true,
    "logs": {
      "enabled": false,
      "head_sampling_rate": 1,
      "invocation_logs": true,
      "persist": true
    },
    "traces": {
      "enabled": false,
      "persist": true,
      "head_sampling_rate": 1
    }
  },
  "r2_buckets": [
    {
      "binding": "R2",
      "bucket_name": "fosdem",
      "preview_bucket_name": "fosdem-preview",
      "remote": true
    }
  ],
  "triggers": {
    "crons": [
      "*/15 8-17 1-2 2 *",
      "0 8-17 * 1 *",
      "0 0 * * *"
    ]
  }
}