{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "fosdem-pwa-build-data",
  "main": "src/index.ts",
  "compatibility_date": "2025-09-02",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "workers_dev": false,
  "preview_urls": false,
  "observability": {
    "enabled": true
  },
  "r2_buckets": [
    {
      "binding": "R2",
      "bucket_name": "fosdem",
      "preview_bucket_name": "fosdem-preview"
    }
  ],
  "triggers": {
    "crons": [
      "*/15 8-17 1-2 2 *",
      "0 8-17 * 1 *",
      "0 0 * * *"
    ]
  },
}