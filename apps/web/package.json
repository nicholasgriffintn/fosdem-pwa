{
  "name": "@fosdem-pwa/web",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "cf-typegen": "wrangler types",
    "dev": "vite dev --port 3000",
    "build:sw": "tsx ./scripts/generate-sw.ts",
    "build": "vite build && pnpm build:sw",
    "preview": "vite preview",
    "start": "node .output/server/index.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "lint": "pnpm biome lint ./src",
    "typecheck": "tsc --noEmit",
    "format": "pnpm biome format --write ./src",
    "db": "drizzle-kit",
    "ui": "pnpm dlx shadcn@latest",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply fosdem --local --persist-to ../cloudflare/state",
    "db:migrate:prod": "wrangler d1 migrations apply fosdem --remote",
    "deploy": "wrangler deploy --env production"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@oslojs/crypto": "1.0.1",
    "@oslojs/encoding": "1.1.0",
    "@radix-ui/react-avatar": "1.1.11",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-scroll-area": "1.2.10",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-slot": "1.2.4",
    "@radix-ui/react-switch": "1.2.6",
    "@radix-ui/react-tabs": "1.1.13",
    "@radix-ui/react-toast": "1.2.15",
    "@radix-ui/react-tooltip": "1.2.8",
    "@sentry/react": "^10.36.0",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "4.1.18",
    "@tanstack/devtools-vite": "0.5.1",
    "@tanstack/react-query": "5.90.21",
    "@tanstack/react-query-devtools": "5.91.3",
    "@tanstack/react-router": "1.160.0",
    "@tanstack/react-router-devtools": "1.160.0",
    "@tanstack/react-router-ssr-query": "1.160.0",
    "@tanstack/react-start": "1.160.0",
    "@tanstack/react-virtual": "^3.13.13",
    "@unpic/react": "1.0.2",
    "arctic": "3.7.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "drizzle-orm": "0.45.1",
    "fuse.js": "7.1.0",
    "hls.js": "1.6.15",
    "lucide-react": "0.564.0",
    "qrcode.react": "4.2.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-resizable-panels": "3.0.6",
    "tailwind-merge": "3.4.0",
    "tailwindcss-animate": "1.0.7",
    "vite": "7.3.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@biomejs/biome": "2.3.15",
    "@cloudflare/vite-plugin": "1.25.0",
    "@cloudflare/workers-types": "4.20260214.0",
    "@playwright/test": "1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "24.10.13",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.4",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "10.4.24",
    "drizzle-kit": "0.31.9",
    "glob": "13.0.3",
    "globals": "17.3.0",
    "jsdom": "27.4.0",
    "tailwindcss": "4.1.18",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.18",
    "wrangler": "4.65.0"
  }
}