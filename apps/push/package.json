{
  "name": "@fosdem-pwa/push",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "wrangler build",
    "deploy": "wrangler deploy",
    "dev": "wrangler dev --test-scheduled --persist-to ../cloudflare/state",
    "start": "wrangler dev --test-scheduled --persist-to ../cloudflare/state",
    "cf-typegen": "wrangler types",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:migrate:local": "wrangler d1 migrations apply fosdem --local --persist-to ../cloudflare/state",
    "db:migrate:prod": "wrangler d1 migrations apply fosdem --remote"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20260210.0",
    "vitest": "4.0.18",
    "typescript": "5.9.3",
    "wrangler": "4.64.0"
  },
  "dependencies": {
    "@sentry/cloudflare": "10.38.0",
    "webpush-webcrypto": "1.0.5"
  }
}